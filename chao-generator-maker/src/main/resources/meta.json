{
  "name": "acm-template-pro-generator",
  "description": "ACM 事例模板生成器",
  "basePackage": "com.chao",
  "version": "0.0.1",
  "author": "chao",
  "gitAddress": "https://github.com/chaoY-1999/chao-generator/",
  "createTime": "2024-02-20",
  "fileConfig": {
    "inputRootPath": ".source/acm-template-pro",
    "outputRootPath": "generated",
    "sourceRootPath": "/Users/chao/chao/custom-project/chao-generator/chao-generator-demo-projects/acm-template-pro",
    "type": "dir",
    "files": [
      {
        "inputPath": "src/com/chao/acm/MainTemplate.java.ftl",
        "outputPath": "src/com/chao/acm/MainTemplate.java",
        "type": "file",
        "generateType": "dynamic"
      },
      {
        "inputPath": ".gitignore",
        "outputPath": ".gitignore",
        "type": "file",
        "generateType": "static"
      },
      {
        "inputPath": "README.md",
        "outputPath": "README.md",
        "type": "file",
        "generateType": "static"
      }
    ]
  },
  "modelConfig": {
    "models": [
      {
        "fieldName": "loop",
        "type": "boolean",
        "description": "是否循环生成",
        "defaultValue": false,
        "abbr": "l"
      },
      {
        "fieldName": "author",
        "type": "String",
        "description": "作者",
        "defaultValue": "chao",
        "abbr": "a"
      },
      {
        "fieldName": "outputText",
        "type": "String",
        "description": "输出文本",
        "defaultValue": "sum = ",
        "abbr": "o"
      }
    ]
  }
}